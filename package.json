{
  "name": "inconnu-learn",
  "version": "1.0.0",
  "description": "INCONNU LEARN - JavaScript learning platform",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build": "cd client && npm install && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "client": "cd client && npm start",
    "dev-all": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "axios": "^1.3.0",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "javascript",
    "learning",
    "education",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Inconnu Boy",
  "license": "MIT"
  }
